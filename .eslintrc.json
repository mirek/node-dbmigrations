{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "array-bracket-spacing": [ "error", "always" ],
    "comma-dangle": [ "error", "never" ],
    "max-len": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": [ "error", "WithStatement" ],
    "padded-blocks": "off"
  }
}
