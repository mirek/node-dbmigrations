{
  "name": "dbmigrations",
  "version": "0.0.4",
  "description": "Database migrations.",
  "main": "lib/index.js",
  "repository": "https://github.com/mirek/node-dbmigrations.git",
  "bin": {
    "dbmigrations": "bin/dbmigrations"
  },
  "scripts": {
    "prepublish": "rm -rf lib && babel src -d lib",
    "test": "npm run prepublish && mocha --check-leaks --reporter spec --require babel-polyfill --compilers js:babel-register spec/",
    "babel": "babel",
    "show-pack": "npm pack && tar -tf dbmigrations-*.tgz | sort && rm dbmigrations-*.tgz",
    "lint": "eslint src/*"
  },
  "keywords": [
    "db",
    "migrations",
    "migrate",
    "postgres",
    "schema"
  ],
  "author": "Mirek Rusin (http://github.com/mirek)",
  "license": "MIT",
  "dependencies": {
    "ajv": "^3.1.1",
    "babel-polyfill": "^6.3.14",
    "chalk": "^1.1.1",
    "debug": "^2.2.0",
    "ejs": "^2.3.4",
    "lodash": "^3.10.1",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2",
    "pgpass": "0.0.3",
    "sequelize": "^3.17.1",
    "sprintf": "^0.1.5",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "fs-extra": "^0.26.4",
    "glob": "^6.0.3",
    "mocha": "^2.3.4"
  }
}
