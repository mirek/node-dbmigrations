{
  "name": "dbmigrations",
  "version": "1.0.0",
  "description": "Database migrations.",
  "main": "lib/index.js",
  "repository": "https://github.com/mirek/node-dbmigrations.git",
  "bin": {
    "dbmigrations": "bin/dbmigrations"
  },
  "scripts": {
    "prepublish": "rm -rf lib && babel src -d lib",
    "test": "npm run prepublish && mocha --check-leaks --reporter spec --require babel-polyfill --compilers js:babel-register spec/",
    "babel": "babel",
    "pack-ls": "npm pack && tar -tf dbmigrations-*.tgz | sort && rm dbmigrations-*.tgz",
    "lint": "eslint src/*"
  },
  "keywords": [
    "db",
    "migrations",
    "migrate",
    "postgres",
    "schema"
  ],
  "author": "Mirek Rusin (http://github.com/mirek)",
  "license": "MIT",
  "dependencies": {
    "ajv": "^4.10.0",
    "babel-polyfill": "^6.20.0",
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "debug": "^2.4.1",
    "ejs": "^2.5.5",
    "lodash": "^4.12.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "pgpass": "1.0.1",
    "sequelize": "^3.27.0",
    "sprintf": "^0.1.5",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "fs-extra": "^1.0.0",
    "glob": "^7.0.3",
    "mocha": "^3.2.0"
  }
}
